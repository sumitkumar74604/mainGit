< <script>
   var a=prompt("enter u name");
   alert(a);
</script> 
<script>
   var value= prompt("enter u city","indore");
   document.write(value);
</script> 
<script>
    var value=prompt("enter u name");
    alert("welcome"+value);
</script> 
 <script>
    var a=prompt("enter a");
    var b=prompt("enter b");
    var c=parseInt(a)+parseInt(b);
    document.write(c);
</script> 
<script>
    var r=parseInt(prompt("enter value of r"));
    var pi=parseFloat(prompt("enter value pi"));
    var ans=pi*r*r;
    document.write(ans);
</script>
<script>
    var f=0;
    var s=1;
    var t=0;
    for(var i=1;i<=8;i++)
    {
        t=f+s;
        document.write(f+",");
        f=s;
        s=t;
    }
</script> 
 <script>
    var n=parseInt(prompt("enter any no"));
    
    var r=0;
    var s=0;
    while(n>0)
    {
        r=n%10;
        s=(s*10)+r;
        n=parseInt(n/10);   
    }
    document.write(s);
</script> 